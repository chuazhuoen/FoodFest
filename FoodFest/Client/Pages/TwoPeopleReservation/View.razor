@page "/twopeoplereservations/view/{id:int}"
@inject HttpClient _client
@inject NavigationManager _navManager
<h3>View</h3>
<FormComponent twoPeopleReservation="twoPeopleReservation" Disabled="true" />
@code {
    [Parameter] public int id { get; set; }
    TwoPeopleReservation twoPeopleReservation = new TwoPeopleReservation();
    protected async override Task OnParametersSetAsync()
        {
            twoPeopleReservation = await
           _client.GetFromJsonAsync<TwoPeopleReservation>($"{Endpoints.TwoPeopleReservationsEndpoint}/{id}");
        }
}
